{
  "name": "agentshop-api",
  "version": "1.0.0",
  "description": "AgentShop API - Domain-neutral demo backend for VorteeGo PetShop",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
