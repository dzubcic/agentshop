    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "pino": "^9.5.0",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "prisma": "^6.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
